{
  "name": "vite-project",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile dist/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile dist/smui-dark.css -i src/theme/dark",
    "dev": "vite --host --clearScreen false",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write ./src/**/*.{svelte,ts}",
    "lint": "eslint './src/**/*.{js,ts,svelte}'",
    "lint:fix": "eslint --fix './src/**/*.{js,ts,svelte}'",
    "prelint": "npm run format",
    "allbuild": "npm run check && npm run prepare && npm run build",
    "allbuildandrun": "npm run allbuild && npm run preview",
    "test": "vitest"
  },
  "devDependencies": {
    "@smui-extra/autocomplete": "^6.0.0-beta.16",
    "@smui/button": "^6.0.0-beta.16",
    "@smui/checkbox": "^6.0.0-beta.16",
    "@smui/circular-progress": "^6.0.0-beta.16",
    "@smui/form-field": "^6.0.0-beta.16",
    "@smui/list": "^6.0.0-beta.16",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/svelte": "^3.1.3",
    "@testing-library/user-event": "^14.2.1",
    "@tsconfig/svelte": "^2.0.1",
    "@types/jest": "^28.1.2",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "jest": "^28.1.1",
    "jsdom": "^20.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "smui-theme": "^6.0.0-beta.16",
    "source-map": "^0.8.0-beta.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-preprocess": "^4.9.8",
    "ts-jest": "^28.0.5",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vitest": "^0.15.1"
  },
  "dependencies": {
    "@smui/select": "^6.0.0-beta.16",
    "@smui/switch": "^6.0.0-beta.16",
    "@smui/tab": "^6.0.0-beta.16",
    "@smui/tab-bar": "^6.0.0-beta.16"
  }
}
