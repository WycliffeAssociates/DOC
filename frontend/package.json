{
  "name": "vite-project",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --clearScreen false",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write ./src/**/*.{svelte,ts}",
